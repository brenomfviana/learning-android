<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.ufrn.edu.brenov.homework8">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ex1.Ex1Activity" />
        <activity android:name=".ex2.Ex2Activity" />
        <activity android:name=".ex1.ResultActivity" />

        <service
            android:name=".ex1.PICalculatorService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".ex2.RandomizerService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>